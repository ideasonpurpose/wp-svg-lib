{
  "name": "ideasonpurpose/wp-svg-lib",
  "description": "Utilities for collecting and reusing inline SVG components",
  "license": "MIT",
  "authors": [
    {
      "name": "Joe Maller",
      "email": "joe@ideasonpurpose.com",
      "homepage": "https://www.ideasonpurpose.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "IdeasOnPurpose\\WP\\": "src"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10",
    "php-stubs/wordpress-stubs": "^5.8",
    "php-stubs/acf-pro-stubs": "^5.10.2",
    "ideasonpurpose/wp-test-stubs": "dev-main@dev"
  },
  "require": {
    "kint-php/kint": "^4",
    "doctrine/inflector": "^2.0"
  },
  "archive": {
    "exclude": [
      "/.git*",
      "/tests",
      "CHANGELOG.md",
      "docker-compose.yml",
      "Dockerfile",
      "package*",
      "phpunit.xml"
    ]
  }
}
